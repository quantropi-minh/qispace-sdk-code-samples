<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qispace_sdk_java_demo">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">

<!--        android:theme="@style/Theme.qispace_sdk_java_demo">-->
        <activity android:name="com.example.qispace_sdk_java_demo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name = "com.example.qispace_sdk_java_demo.SequrActivity"
            android:label = "Sequr Page"
            android:theme = "@style/Theme.AppCompat"/>

        <activity android:name = "com.example.qispace_sdk_java_demo.QeepActivity"
            android:label = "Qeep Page"
            android:theme = "@style/Theme.AppCompat"/>

        <activity android:name = "com.example.qispace_sdk_java_demo.MasqActivity"
            android:label = "Masq Page"
            android:theme = "@style/Theme.AppCompat"/>
    </application>

</manifest>